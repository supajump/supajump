{
  "name": "@supajump/create-app",
  "version": "0.3.0",
  "description": "Create a new Supajump app - Multi-tenant SaaS starter with Next.js and Supabase",
  "type": "module",
  "bin": {
    "create-supajump-app": "./dist/index.js"
  },
  "scripts": {
    "dev": "tsup src/index.ts --watch",
