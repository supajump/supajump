  "keywords": [
    "supajump",
    "supabase",
    "nextjs",
    "saas",
    "starter",
    "template",
    "multi-tenant"
  ],
  "author": "Supajump",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/supajump/supajump.git",
    "directory": "packages/create-supajump-app"
  },
  "bugs": {
    "url": "https://github.com/supajump/supajump/issues"
  },
  "homepage": "https://github.com/supajump/supajump#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@clack/prompts": "^0.8.2",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "execa": "^9.5.2",
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.10.2",
